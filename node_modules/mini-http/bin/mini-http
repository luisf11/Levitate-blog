#!/usr/bin/env node

'use strict';

var miniHttp     = require('../index.js');
var commands     = require('../lib/commands.js');
var cliFilter    = require('../lib/cli-filter.js');

var config = {
  help: false,
  start: true
};

config = cliFilter(process.argv, config, commands);

if (config.start) return miniHttp.serve(config);
